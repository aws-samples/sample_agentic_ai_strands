# AWS Cognito Configuration
# These values should be obtained from the agentcore_scripts/utils.py setup_cognito_user_pool() function output

# AWS Region where the Cognito UserPool is created
NEXT_PUBLIC_AWS_REGION=

# Cognito UserPool ID - replace with actual value from setup_cognito_user_pool() output
NEXT_PUBLIC_COGNITO_USER_POOL_ID=

# Cognito UserPool Client ID - replace with actual value from setup_cognito_user_pool() output
NEXT_PUBLIC_COGNITO_USER_POOL_CLIENT_ID=

# Cognito UserPool Domain (optional, for hosted UI)
# NEXT_PUBLIC_COGNITO_DOMAIN=your_domain_here

# API Key for authentication (existing)
NEXT_PUBLIC_API_KEY=123456

# Base URL for MCP service - Server side (internal)
SERVER_MCP_BASE_URL=http://localhost:7002

# Base URL for MCP service - Client side (now uses Next.js API routes)
NEXT_PUBLIC_MCP_BASE_URL=/api

# Base URL for direct client-side API access (used for WebSockets)
NEXT_PUBLIC_API_BASE_URL=https://ip:7002

# Test user credentials (from agentcore_scripts/utils.py)
NEXT_PUBLIC_TEST_USERNAME=testuser
NEXT_PUBLIC_TEST_PASSWORD=MyPassword123!
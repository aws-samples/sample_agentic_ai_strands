# Strands Agents SDK Configuration Example
# Copy this file to .env and modify the values as needed

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================

LOG_DIR=./logs
CHATBOT_SERVICE_PORT=8502
MCP_SERVICE_HOST=0.0.0.0
MCP_SERVICE_PORT=7002
CLIENT_TYPE=strands
USE_HTTPS=0
# ddb for user config
ddb_table=agent_user_config_table
# for Development mode - API Key for server authentication. For production, it uses cognito
API_KEY=123456

# =============================================================================
# COGNITO AUTHENTICATION CONFIGURATION
# AWS Cognito UserPool configuration for JWT token authentication
# you can find them from agentcore_scrpts/.env_cognito
#COGNITO_USER_POOL_ID=<pool_id>
#COGNITO_CLIENT_ID=<app_client_id>
#COGNITO_M2M_CLIENT_ID=<m2m_client_id>
#COGNITO_M2M_CLIENT_SECRET=<m2m_client_secret>
#COGNITO_M2M_CLIENT_SCOPE=<scope_string>
# =============================================================================
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=
COGNITO_M2M_CLIENT_ID=
COGNITO_M2M_CLIENT_SECRET=
COGNITO_M2M_CLIENT_SCOPE=


# =============================================================================
# AWS Infra CONFIGURATION
# The default ECS platform is amd64, you can choose linux/amd64  or  linux/arm64
# =============================================================================
PLATFORM=linux/arm64
AWS_REGION=us-west-2
#AWS_ACCESS_KEY_ID=
#AWS_SECRET_ACCESS_KEY=

# =============================================================================
# AGENTCORE CONFIGURATION
# =============================================================================
AGENTCORE_REGION=us-west-2
MEMORY_ID=
AGENTCORE_RUNTIME_ARN=
NOVA_ACT_API_KEY=
# =============================================================================
# STRANDS AGENT CONFIGURATION
# =============================================================================
# Model provider: bedrock, openai
STRANDS_MODEL_PROVIDER=bedrock

# API Key (required for OpenAI)
# OPENAI_API_KEY=your_api_key_here

# API Base URL (for OpenAI-compatible APIs)
# OPENAI_BASE_URL=https://api.openai.com/v1

# =============================================================================
# AWS BEDROCK CONFIGURATION (for Bedrock provider, if not set, it will use same credential as AWS Infra)
# =============================================================================

# BEDROCK_AWS_ACCESS_KEY_ID=your_aws_access_key_id
# BEDROCK_AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
# BEDROCK_AWS_REGION=us-west-2

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================
# Session inactive time (minutes)
INACTIVE_TIME=15

# =============================================================================
# LANGFUSE CONFIGURATION of strands
# =============================================================================
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=

# =============================================================================
# ADDITIONAL SETTINGS
# =============================================================================

# =============================================================================
# mem0 CONFIGURATION
# Only used if ENABLE_MEM0=true
# If STRANDS_MODEL_PROVIDER=bedrock, it will use models in Bedrock
# =============================================================================
ENABLE_MEM0=false
# LLM_MODEL="Qwen/Qwen3-14B"
# EMBEDDING_MODEL="Pro/BAAI/bge-m3"
# LLM_MODEL=us.amazon.nova-pro-v1:0
# EMBEDDING_MODEL=amazon.titan-embed-text-v2:0
